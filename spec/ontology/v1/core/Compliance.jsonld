        "@id": "dppk:Product"
      },
      "rdfs:range": {
        "@id": "dppk:Packaging"
      },
      "owl:equivalentProperty": {
        "@id": "gs1:packaging"
      },
      "rdfs:term_status": "unstable"
    },
    {
      "@id": "dppk:productionSteps",
