{
  "@context": {
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "schema": "https://schema.org/",
    "gs1": "https://ref.gs1.org/voc/",
    "dppk": "https://dpp-keystone.org/spec/v1/terms#",
    "dcterms": "http://purl.org/dc/terms/",
    "unece": "https://vocabulary.uncefact.org/",
    "@base": "https://dpp-keystone.org/spec/v1/terms"
  },
  "@id": "https://dpp-keystone.org/spec/ontology/v1/core/Compliance",
  "@type": "owl:Ontology",
  "dcterms:title": "DPP Core Ontology - Compliance and Regulatory Definitions (v1)",
  "dcterms:description": "Defines horizontal concepts related to Documents, Certifications, Substances of Concern, Eco-Design, Packaging, and Production Steps.",
  "owl:versionInfo": "1.0.0",
  "owl:imports": [
    {
      "@id": "https://dpp-keystone.org/spec/ontology/v1/core/Product.jsonld"
    },
    {
      "@id": "https://dpp-keystone.org/spec/ontology/v1/core/Organization.jsonld"
    },
    {
      "@id": "https://dpp-keystone.org/spec/ontology/v1/core/RelatedResource.jsonld"
    }
  ],
  "@graph": [
    {
      "@id": "dppk:Certification",
      "@type": "rdfs:Class",
      "rdfs:label": "Certification",
      "rdfs:comment": "A confirmation that the product meets the requirements of a specific standard or regulation (e.g., CE Marking, EPD).",
      "rdfs:subClassOf": {
        "@id": "dppk:RelatedResource"
      },
      "owl:equivalentClass": [
        {
          "@id": "schema:Certification"
        },
        {
          "@id": "gs1:CertificationDetails"
        },
        {
          "@id": "unece:certificationEvidenceDocument"
        }
      ]
    },
    {
      "@id": "dppk:SubstanceOfConcern",
      "@type": "rdfs:Class",
      "rdfs:label": "Substance of Concern",
      "rdfs:comment": "Details regarding dangerous substances or substances of very high concern (SVHC) present in the product."
    },
    {
      "@id": "dppk:Packaging",
      "@type": "rdfs:Class",
      "rdfs:label": "Packaging",
      "rdfs:comment": "Information about the materials and properties of the product's packaging.",
      "owl:equivalentClass": [
        {
          "@id": "gs1:PackagingMaterialDetails"
        }
      ]
    },
    {
      "@id": "dppk:ProductionStep",
      "@type": "rdfs:Class",
      "rdfs:label": "Production Step",
      "rdfs:comment": "A significant step in the manufacturing or processing chain of the product, including its location."
    },
    {
      "@id": "dppk:documents",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "Documents",
      "rdfs:comment": "Relates the product to associated digital documents.",
      "rdfs:domain": {
        "@id": "dppk:Product"
      },
      "rdfs:range": {
        "@id": "dppk:RelatedResource"
      },
      "owl:equivalentProperty": {
        "@id": "gs1:referencedFile"
      }
    },
    {
      "@id": "dppk:additionalCertifications",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "Additional Certifications. Please do not use this field for sector specific, mandatory certifications.",
      "rdfs:comment": "Relates the product to its certifications.",
      "rdfs:domain": {
        "@id": "dppk:Product"
      },
      "rdfs:range": {
        "@id": "dppk:Certification"
      },
      "owl:equivalentProperty": [
        {
          "@id": "schema:hasCertification"
        },
        {
          "@id": "gs1:certification"
        }
      ]
    },
    {
      "@id": "dppk:substancesOfConcern",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "Substances of Concern",
      "rdfs:comment": "Relates the product to any substances of concern it contains.",
      "rdfs:domain": {
        "@id": "dppk:Product"
      },
      "rdfs:range": {
        "@id": "dppk:SubstanceOfConcern"
      }
    },
    {
      "@id": "dppk:packaging",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "Packaging",
      "rdfs:comment": "A list of packaging elements associated with the product.",
      "rdfs:domain": {
        "@id": "dppk:Product"
      },
      "rdfs:range": {
        "@id": "dppk:Packaging"
      },
      "owl:equivalentProperty": {
        "@id": "gs1:packaging"
      }
    },
    {
      "@id": "dppk:productionSteps",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "Production Steps",
      "rdfs:domain": {
        "@id": "dppk:Product"
      },
      "rdfs:range": {
        "@id": "dppk:ProductionStep"
      }
    },
    {
      "@id": "dppk:certificationBodyName",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "Certifying Body Name",
      "rdfs:comment": "The name of the body that issued the certification.",
      "rdfs:domain": {
        "@id": "dppk:Certification"
      },
      "owl:equivalentProperty": {
        "@id": "gs1:certificationAgency"
      }
    },
    {
      "@id": "dppk:certificationBodyId",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "Certifying Body ID",
      "rdfs:comment": "The unique identifier of the certifying body.",
      "rdfs:domain": {
        "@id": "dppk:Certification"
      },
      "rdfs:range": {
        "@id": "xsd:string"
      }
    },
    {
      "@id": "dppk:certificationStartDate",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "Start of Certificate Validity",
      "rdfs:comment": "The date when the certification became valid.",
      "rdfs:domain": {
        "@id": "dppk:Certification"
      },
      "rdfs:range": {
        "@id": "xsd:date"
      },
      "owl:equivalentProperty": {
        "@id": "gs1:certificationStartDate"
      }
    },
    {
      "@id": "dppk:minValue",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "Minimum Value",
      "rdfs:comment": "The minimum concentration or amount of the substance present.",
      "rdfs:domain": {
        "@id": "dppk:SubstanceOfConcern"
      },
      "owl:equivalentProperty": {
        "@id": "schema:minValue"
      }
    },
    {
      "@id": "dppk:maxValue",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "Maximum Value",
      "rdfs:comment": "The maximum concentration or amount of the substance present.",
      "rdfs:domain": {
        "@id": "dppk:SubstanceOfConcern"
      },
      "owl:equivalentProperty": {
        "@id": "schema:maxValue"
      }
    },
    {
      "@id": "dppk:substanceName",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "Substance Name",
      "rdfs:comment": "The common name of the substance.",
      "rdfs:domain": {
        "@id": "dppk:SubstanceOfConcern"
      },
      "rdfs:range": {
        "@id": "xsd:string"
      }
    },
    {
      "@id": "dppk:casNumber",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "CAS Number",
      "rdfs:comment": "The Chemical Abstracts Service (CAS) registry number for the substance.",
      "dppk:unit": "unitless",
      "rdfs:domain": {
        "@id": "dppk:SubstanceOfConcern"
      },
      "rdfs:range": {
        "@id": "xsd:string"
      }
    },
    {
      "@id": "dppk:concentration",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "Concentration",
      "rdfs:comment": "The concentration of the substance in the product.",
      "dppk:unit": "%",
      "rdfs:domain": {
        "@id": "dppk:SubstanceOfConcern"
      },
      "rdfs:range": {
        "@id": "xsd:double"
      }
    },
    {
      "@id": "dppk:packagingMaterialType",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "Packaging Material Type",
      "rdfs:comment": "The type of material used for the packaging (e.g., 'Cardboard', 'Plastic').",
      "rdfs:domain": {
        "@id": "dppk:Packaging"
      },
      "owl:equivalentProperty": {
        "@id": "gs1:packagingMaterialType"
      }
    },
    {
      "@id": "dppk:packagingMaterialCompositionQuantity",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "Packaging Material Composition Quantity",
      "rdfs:comment": "The percentage of the material in the packaging.",
      "rdfs:domain": {
        "@id": "dppk:Packaging"
      },
      "owl:equivalentProperty": {
        "@id": "gs1:packagingMaterialCompositionQuantity"
      },
      "dppk:unit": "%"
    },
    {
      "@id": "dppk:packagingRecyclingProcessType",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "Packaging Recycling Process Type",
      "rdfs:comment": "Indicates if the packaging is RECYCLABLE, REUSABLE, or COMPOSTABLE.",
      "rdfs:domain": {
        "@id": "dppk:Packaging"
      },
      "owl:equivalentProperty": {
        "@id": "gs1:packagingRecyclingProcessType"
      }
    },
    {
      "@id": "dppk:packagingRecycledContent",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "Packaging Recycled Content",
      "rdfs:comment": "The percentage of recycled content in the packaging material.",
      "rdfs:domain": {
        "@id": "dppk:Packaging"
      }
    },
    {
      "@id": "dppk:packagingSubstanceOfConcern",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "Packaging Substance of Concern Present",
      "rdfs:comment": "Indicates if any substances of concern are present in the packaging.",
      "rdfs:domain": {
        "@id": "dppk:Packaging"
      }
    },
    {
      "@id": "dppk:productionStepType",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "Production Step Type",
      "rdfs:comment": "The name or description of the production step (e.g., Assembly, Weaving, Smelting).",
      "rdfs:domain": {
        "@id": "dppk:ProductionStep"
      }
    },
    {
      "@id": "dppk:productionLocationCountry",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "Production Location Country",
      "rdfs:comment": "The country where the production step took place. Recommended to use ISO 3166-1 alpha-3 codes.",
      "rdfs:domain": {
        "@id": "dppk:ProductionStep"
      },
      "owl:equivalentProperty": {
        "@id": "gs1:countryCode"
      }
    }
  ]
}
