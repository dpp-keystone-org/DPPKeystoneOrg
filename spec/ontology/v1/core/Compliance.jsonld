{
  "@context": {
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "schema": "https://schema.org/",
    "gs1": "https://ref.gs1.org/voc/",
    "dppk": "https://dpp-keystone.org/spec/v1/terms#",
    "dcterms": "http://purl.org/dc/terms/",
    "unece": "https://vocabulary.uncefact.org/",
    "@base": "https://dpp-keystone.org/spec/v1/terms"
  },
  "@id": "https://dpp-keystone.org/ontology/v1/core/Compliance",
  "@type": "owl:Ontology",
  "dcterms:title": "DPP Core Ontology - Compliance and Regulatory Definitions (v1)",
  "dcterms:description": "Defines horizontal concepts related to Documents, Certifications, Substances of Concern, Eco-Design, Packaging, and Production Steps.",
  "owl:versionInfo": "1.0.0",
  "owl:imports": [
    {
      "@id": "https://dpp-keystone.org/ontology/v1/core/Product.jsonld"
    },
    {
      "@id": "https://dpp-keystone.org/ontology/v1/core/Organization.jsonld"
    }
  ],
  "@graph": [
    {
      "@id": "dppk:DigitalDocument",
      "@type": "rdfs:Class",
      "rdfs:label": "Digital Document",
      "rdfs:comment": "A reference to an external digital file related to the product (e.g., manuals, reports, declarations).",
      "owl:equivalentClass": [
        {
          "@id": "schema:DigitalDocument"
        },
        {
          "@id": "gs1:ReferencedFile"
        },
        {
          "@id": "unece:Document"
        }
      ],
      "rdfs:term_status": "unstable"
    },
    {
      "@id": "dppk:Certification",
      "@type": "rdfs:Class",
      "rdfs:label": "Certification",
      "rdfs:comment": "A confirmation that the product meets the requirements of a specific standard or regulation (e.g., CE Marking, EPD).",
      "owl:equivalentClass": [
        {
          "@id": "schema:Certification"
        },
        {
          "@id": "gs1:CertificationDetails"
        },
        {
          "@id": "unece:certificationEvidenceDocument"
        }
      ],
      "rdfs:term_status": "unstable"
    },
    {
      "@id": "dppk:SubstanceOfConcern",
      "@type": "rdfs:Class",
      "rdfs:label": "Substance of Concern",
      "rdfs:comment": "Details regarding dangerous substances or substances of very high concern (SVHC) present in the product.",
      "rdfs:term_status": "unstable"
    },
    {
      "@id": "dppk:EcoDesignProperty",
      "@type": "rdfs:Class",
      "rdfs:label": "Eco-Design Property",
      "rdfs:comment": "A specific property related to the environmental performance or sustainability of the product, as required by Eco-Design regulations (e.g., Energy Efficiency, GWP).",
      "dcterms:source": {
        "@id": "https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:32009L0125",
        "rdfs:label": "Directive 2009/125/EC (Ecodesign Directive)"
      },
      "rdfs:subClassOf": {
        "@id": "dppk:QuantitativeValue"
      },
      "rdfs:term_status": "unstable"
    },
    {
      "@id": "dppk:ComplianceTopic",
      "@type": "rdfs:Class",
      "rdfs:label": "Compliance Topic",
      "rdfs:comment": "A declaration of compliance status regarding specific regulations (e.g., REACH, RoHS, Lacey Act).",
      "rdfs:term_status": "unstable"
    },
    {
      "@id": "dppk:Packaging",
      "@type": "rdfs:Class",
      "rdfs:label": "Packaging",
      "rdfs:comment": "Information about the materials and properties of the product's packaging.",
      "owl:equivalentClass": [
        {
          "@id": "gs1:PackagingMaterialDetails"
        }
      ],
      "rdfs:term_status": "unstable"
    },
    {
