{
  "@context": {
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "schema": "https://schema.org/",
    "gs1": "https://ref.gs1.org/voc/",
    "dppk": "https://dpp-keystone.org/spec/v1/terms#",
    "dcterms": "http://purl.org/dc/terms/",
    "unece": "https://vocabulary.uncefact.org/",
    "@base": "https://dpp-keystone.org/spec/v1/terms"
  },
  "@id": "https://dpp-keystone.org/spec/ontology/v1/core/Header.jsonld",
  "@type": "owl:Ontology",
  "dcterms:title": "DPP Core Ontology - Header Definitions (v1)",
  "dcterms:description": "Defines the normative header fields required for the Digital Product Passport envelope.",
  "owl:versionInfo": "1.0.0",
  "@graph": [
    {
      "@id": "dppk:DigitalProductPassport",
      "@type": "rdfs:Class",
      "rdfs:label": "Digital Product Passport (DPP)",
      "rdfs:comment": "A structured collection of product-related data defining the characteristics, provenance, and lifecycle information of a product, as mandated by the Ecodesign for Sustainable Products Regulation (ESPR).",
      "dcterms:source": {
        "@id": "https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52022PC0142",
        "rdfs:label": "Proposal for Ecodesign for Sustainable Products Regulation (ESPR)"
      },
      "rdfs:subClassOf": [
        {
          "@id": "schema:Product"
        },
        {
          "@id": "schema:Certification"
        },
        {
          "@id": "unece:Document"
        }
      ],
      "rdfs:term_status": "unstable"
    },
    {
      "@id": "dppk:digitalProductPassportId",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "DPP Identifier",
      "rdfs:comment": "The unique identifier for this specific Digital Product Passport instance. Should be a persistent, globally unique, and resolvable URI (e.g., URN UUID or HTTP URI).",
      "rdfs:domain": {
        "@id": "dppk:DigitalProductPassport"
      },
      "rdfs:range": {
        "@id": "xsd:anyURI"
      },
      "rdfs:term_status": "unstable"
    },
    {
      "@id": "dppk:uniqueProductIdentifier",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "Unique Product Identifier (UPI)",
      "rdfs:comment": "The unique identifier for the physical product associated with this DPP. Aligns with the Unique Product Identifier (UPI) requirement in the ESPR. Strongly recommended to use established standards like GS1 Digital Link URI.",
      "dcterms:source": {
        "@id": "https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:52022PC0142",
        "rdfs:label": "ESPR - Article 9 (Unique product identifier)"
      },
      "rdfs:domain": {
        "@id": "dppk:DigitalProductPassport"
      },
      "rdfs:range": {
        "@id": "xsd:anyURI"
      },
      "owl:equivalentProperty": [
        {
          "@id": "gs1:productID"
        },
        {
          "@id": "schema:productID"
        }
      ],
      "rdfs:term_status": "unstable"
    },
    {
      "@id": "dppk:economicOperatorId",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "Economic Operator Identifier (EOI)",
      "rdfs:comment": "The unique identifier for the Economic Operator responsible for placing the product on the market.",
      "rdfs:domain": {
        "@id": "dppk:DigitalProductPassport"
      },
      "rdfs:range": {
        "@id": "xsd:anyURI"
      },
      "rdfs:term_status": "unstable"
    },
    {
      "@id": "dppk:facilityId",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "Facility Identifier",
      "rdfs:comment": "Optional. The unique identifier for the manufacturing or processing facility associated with the product.",
      "rdfs:domain": {
        "@id": "dppk:DigitalProductPassport"
      },
      "rdfs:range": {
        "@id": "xsd:anyURI"
      },
      "rdfs:term_status": "unstable"
    },
    {
      "@id": "dppk:GranularityValue",
      "@type": "rdfs:Class",
      "rdfs:label": "Granularity Value",
      "rdfs:comment": "An enumeration of the allowed granularity levels for a DPP.",
      "rdfs:subClassOf": {
        "@id": "rdfs:Resource"
      },
      "owl:oneOf": [
        {
          "@id": "dppk:Model"
        },
        {
          "@id": "dppk:Batch"
        },
        {
          "@id": "dppk:Item"
        }
      ]
    },
    {
      "@id": "dppk:Model",
      "@type": "dppk:GranularityValue"
    },
    {
      "@id": "dppk:Batch",
      "@type": "dppk:GranularityValue"
    },
    {
      "@id": "dppk:Item",
      "@type": "dppk:GranularityValue"
    },
    {
      "@id": "dppk:granularity",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "Granularity Level",
      "rdfs:comment": "Indicates the level of detail the DPP applies to: 'Model' (product type), 'Batch' (production lot), or 'Item' (individual serialized unit).",
      "rdfs:domain": {
        "@id": "dppk:DigitalProductPassport"
      },
      "rdfs:range": {
        "@id": "dppk:GranularityValue"
      },
      "rdfs:term_status": "unstable"
    },
    {
      "@id": "dppk:dppStatus",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "DPP Status",
      "rdfs:comment": "The current lifecycle status of the DPP instance (e.g., 'active', 'inactive').",
      "rdfs:domain": {
        "@id": "dppk:DigitalProductPassport"
      },
      "rdfs:range": {
        "@id": "xsd:string"
      },
      "rdfs:term_status": "unstable"
    },
    {
      "@id": "dppk:lastUpdate",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "Last Update Timestamp",
      "rdfs:comment": "The date and time when the DPP instance was last modified. Must be in ISO 8601 format (xsd:dateTime).",
      "rdfs:domain": {
        "@id": "dppk:DigitalProductPassport"
      },
      "rdfs:range": {
        "@id": "xsd:dateTime"
      },
      "rdfs:term_status": "unstable"
    },
    {
      "@id": "dppk:versionNumber",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "Version Number",
      "rdfs:comment": "The version number of the DPP instance.",
      "rdfs:domain": {
        "@id": "dppk:DigitalProductPassport"
      },
      "rdfs:range": {
        "@id": "xsd:string"
      },
      "rdfs:term_status": "unstable"
    },
    {
      "@id": "dppk:versionDate",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "Version Date",
      "rdfs:comment": "The date of the version of the DPP instance.",
      "rdfs:domain": {
        "@id": "dppk:DigitalProductPassport"
      },
      "rdfs:range": {
        "@id": "xsd:date"
      },
      "rdfs:term_status": "unstable"
    },
    {
      "@id": "dppk:dppSchemaVersion",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "DPP Schema Version",
      "rdfs:comment": "The version of the horizontal (cross-sector) DPP schema used for this instance.",
      "rdfs:domain": {
        "@id": "dppk:DigitalProductPassport"
      },
      "rdfs:range": {
        "@id": "xsd:string"
      },
      "rdfs:term_status": "unstable"
    },
    {
      "@id": "dppk:contentSpecificationIds",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "Content Specification Identifiers",
      "rdfs:comment": "A set of identifiers for the delegated acts or sector-specific data models this DPP conforms to.",
      "rdfs:domain": {
        "@id": "dppk:DigitalProductPassport"
      },
      "rdfs:range": {
        "@id": "xsd:string"
      },
      "rdfs:term_status": "unstable"
    },
    {
      "@id": "dppk:hsCode",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "Harmonized System Code (HS Code) or TARIC",
      "rdfs:comment": "The customs tariff number (e.g., HS Code, TARIC code) used for international trade classification.",
      "rdfs:domain": {
        "@id": "dppk:DigitalProductPassport"
      },
      "rdfs:range": {
        "@id": "xsd:string"
      },
      "owl:equivalentProperty": [
        {
          "@id": "gs1:customsClassification"
        },
        {
          "@id": "schema:hscode"
        }
      ],
      "rdfs:term_status": "unstable"
    }
  ]
}
