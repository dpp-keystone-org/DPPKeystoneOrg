{
  "@context": {
    "sh": "http://www.w3.org/ns/shacl#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "dppk": "https://dpp-keystone.org/spec/v1/terms#",
    "dppkval": "https://dpp-keystone.org/spec/validation/v1/shacl/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "@vocab": "http://www.w3.org/ns/shacl#",
    "node": {
      "@type": "@id"
    },
    "path": {
      "@type": "@id"
    },
    "targetClass": {
      "@type": "@id"
    },
    "class": {
      "@type": "@id"
    },
    "datatype": {
      "@type": "@id"
    }
  },
  "@id": "dppkval:ElectronicsShapes",
  "@type": "ShapesGraph",
  "@graph": [
    {
      "@id": "dppkval:ElectronicDeviceShape",
      "@type": "NodeShape",
      "targetClass": "dppk:ElectronicDevice",
      "and": [
        {
          "node": "dppkval:DigitalProductPassportShape"
        }
      ],
      "property": [
        {
          "path": "dppk:voltage",
          "node": "dppkval:QuantitativeValueShape"
        },
        {
          "path": "dppk:ratedPower",
          "node": "dppkval:QuantitativeValueShape"
        },
        {
          "path": "dppk:sparePartsAvailable",
          "datatype": "xsd:boolean"
        }
      ]
    }
  ]
}
