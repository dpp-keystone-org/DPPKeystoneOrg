{
  "@context": {
    "sh": "http://www.w3.org/ns/shacl#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "dppk": "https://dpp-keystone.org/spec/v1/terms#",
    "dppkval": "https://dpp-keystone.org/spec/validation/v1/shacl/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "@vocab": "http://www.w3.org/ns/shacl#",
    "node": { "@type": "@id" },
    "path": { "@type": "@id" },
    "targetClass": { "@type": "@id" },
    "class": { "@type": "@id" },
    "datatype": { "@type": "@id" }
  },
  "@id": "dppkval:GeneralProductShapes",
  "@type": "ShapesGraph",
  
  "@graph": [
    {
      "@id": "dppkval:GeneralProductShape",
      "@type": "NodeShape",
      "targetClass": "dppk:DigitalProductPassport",
      "property": [
        {
          "path": "dppk:brand",
          "minCount": 0,
          "datatype": "xsd:string"
        },
        {
          "path": "dppk:model",
          "minCount": 0,
          "datatype": "xsd:string"
        },
        {
          "path": "dppk:color",
          "minCount": 0,
          "datatype": "xsd:string"
        },
        {
          "path": "dppk:countryOfOrigin",
          "minCount": 0,
          "datatype": "xsd:string"
        },
        {
          "path": "dppk:netWeight",
          "minCount": 0
        },
        {
          "path": "dppk:grossWeight",
          "minCount": 0,
          "datatype": "xsd:double" 
        },
        {
            "path": "dppk:length",
            "minCount": 0
        },
        {
            "path": "dppk:width",
            "minCount": 0
        },
        {
            "path": "dppk:height",
            "minCount": 0
        },
        {
            "path": "dppk:depth",
            "minCount": 0
        },
        {
            "path": "dppk:components",
            "minCount": 0
            
        }
      ]
    }
  ]
}