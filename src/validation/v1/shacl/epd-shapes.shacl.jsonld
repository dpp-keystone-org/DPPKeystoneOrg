{
  "@context": {
    "sh": "http://www.w3.org/ns/shacl#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "dppk": "https://dpp-keystone.org/spec/v1/terms#",
    "dppkval": "https://dpp-keystone.org/spec/validation/v1/shacl/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "@vocab": "http://www.w3.org/ns/shacl#",
    "node": { "@type": "@id" },
    "path": { "@type": "@id" },
    "targetClass": { "@type": "@id" },
    "class": { "@type": "@id" },
    "datatype": { "@type": "@id" }
  },
  "@id": "dppkval:EPDShapes",
  "@type": "ShapesGraph",
  
  "@graph": [
    {
      "@id": "dppkval:EPDShape",
      "@type": "NodeShape",
      "targetClass": "dppk:EPD",
      "property": [
        {
          "path": "dppk:gwp",
          "node": "dppkval:EPDIndicatorShape",
          "minCount": 0
        },
        {
          "path": "dppk:gwpF",
          "node": "dppkval:EPDIndicatorShape",
          "minCount": 0
        }
      ]
    },
    {
        "@id": "dppkval:EPDIndicatorShape",
        "@type": "NodeShape",
        "property": [
            {
                "path": "dppk:a1",
                "datatype": "xsd:double",
                "minCount": 0
            },
            {
                "path": "dppk:a2",
                "datatype": "xsd:double",
                "minCount": 0
            },
            {
                "path": "dppk:a3",
                "datatype": "xsd:double",
                "minCount": 0
            },
            {
                "path": "dppk:total",
                "datatype": "xsd:double",
                "minCount": 0
            }
        ]
    }
  ]
}
