{
      "@context": {
        "dppk": "https://dpp-keystone.org/spec/v1/terms#",
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",        "xsd": "http://www.w3.org/2001/XMLSchema#",

        // Term for EPD Data Block which holds an array of EPD Indicators
        "epd": {
            "@id": "dppk:epd"
        },

        // EPD Indicators
        "gwp": "dppk:gwp",
        "gwpF": "dppk:gwpF",
        "gwpB": "dppk:gwpB",
        "gwpLuluc": "dppk:gwpLuluc",
        "gwpGhg": "dppk:gwpGhg",
        "odp": "dppk:odp",
        "ap": "dppk:ap",
        "epF": "dppk:epF",
        "epM": "dppk:epM",
        "epT": "dppk:epT",
        "pocp": "dppk:pocp",
        "adpe": "dppk:adpe",
        "adpf": "dppk:adpf",
        "wdp": "dppk:wdp",

        // EPD Life Cycle Stages with Type Coercion
        "a1": { "@id": "dppk:a1", "@type": "xsd:decimal" },
        "A1": { "@id": "dppk:a1", "@type": "xsd:decimal" },
        "a2": { "@id": "dppk:a2", "@type": "xsd:decimal" },
        "A2": { "@id": "dppk:a2", "@type": "xsd:decimal" },
        "a3": { "@id": "dppk:a3", "@type": "xsd:decimal" },
        "A3": { "@id": "dppk:a3", "@type": "xsd:decimal" },
        "a4": { "@id": "dppk:a4", "@type": "xsd:decimal" },
        "A4": { "@id": "dppk:a4", "@type": "xsd:decimal" },
        "c1": { "@id": "dppk:c1", "@type": "xsd:decimal" },
        "C1": { "@id": "dppk:c1", "@type": "xsd:decimal" },
        "c2": { "@id": "dppk:c2", "@type": "xsd:decimal" },
        "C2": { "@id": "dppk:c2", "@type": "xsd:decimal" },
        "c3": { "@id": "dppk:c3", "@type": "xsd:decimal" },
        "C3": { "@id": "dppk:c3", "@type": "xsd:decimal" },
        "c4": { "@id": "dppk:c4", "@type": "xsd:decimal" },
        "C4": { "@id": "dppk:c4", "@type": "xsd:decimal" },
        "d": { "@id": "dppk:d", "@type": "xsd:decimal" },
        "D": { "@id": "dppk:d", "@type": "xsd:decimal" },
        "total": { "@id": "dppk:total", "@type": "xsd:decimal" },
        "Total": { "@id": "dppk:total", "@type": "xsd:decimal" }
    }
}
