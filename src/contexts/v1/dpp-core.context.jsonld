{
  "@context": {
    // ==================================================================
    // Namespaces
    // ==================================================================
    "dppk": "https://dpp-keystone.org/spec/v1/terms#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    // While the ontology defines equivalencies, we primarily map to the harmonized jtc24 terms here.
    // GS1 and Schema.org namespaces are included for potential direct use if needed.
    "gs1": "https://ref.gs1.org/voc/",
    "schema": "https://schema.org/",
    "unece": "https://vocabulary.uncefact.org/",

    // ==================================================================
    // Core Classes (for use in @type)
    // ==================================================================
    "DigitalProductPassport": "dppk:DigitalProductPassport",
    "Organization": "dppk:Organization",
    "PostalAddress": "dppk:PostalAddress",
    "Product": "dppk:Product",
    "QuantitativeValue": "dppk:QuantitativeValue",
    "DigitalDocument": "dppk:DigitalDocument",
    "Certification": "dppk:Certification",
    "SubstanceOfConcern": "dppk:SubstanceOfConcern",
    "EcoDesignProperty": "dppk:EcoDesignProperty",
    "ComplianceTopic": "dppk:ComplianceTopic",
    "Packaging": "dppk:Packaging",
    "ProductionStep": "dppk:ProductionStep",

    // Roles (for co-typing)
    "EconomicOperatorRole": "dppk:EconomicOperatorRole",
    "ManufacturerRole": "dppk:ManufacturerRole",
    "FacilityRole": "dppk:FacilityRole",
    "ConformityAssessmentBodyRole": "dppk:ConformityAssessmentBodyRole",
    "ISO6523": "dppk:ISO6523",

    // ==================================================================
    // Properties: DPP Header (camelCase)
    // ==================================================================
    "digitalProductPassportId": { "@id": "dppk:DPPID", "@type": "@id" },
    "productId": { "@id": "dppk:ProductID", "@type": "@id" },
    "economicOperatorId": { "@id": "dppk:EconomicOperatorID", "@type": "@id" },
    "facilityId": { "@id": "dppk:FacilityID", "@type": "@id" },
    "granularity": "dppk:Granularity",
    "dppStatus": "dppk:StatusOfTheDPP",
    "lastUpdated": { "@id": "dppk:lastUpdated", "@type": "xsd:dateTime" },
    "dppSchemaVersion": "dppk:dppSchemaVersion",
    "delegatedActIds": {
      "@id": "dppk:delegatedActIds",
      "@container": "@set"
    },
    "verifiableCredential": "dppk:verifiableCredential",
    "hsCode": "dppk:hsCode",

    // ==================================================================
    // Properties: Organization and Roles (camelCase)
    // ==================================================================
    
    // Relationships
    "economicOperator": { "@id": "dppk:economicOperator", "@type": "@id" },
    "manufacturer": { "@id": "dppk:manufacturer", "@type": "@id" },
    "manufacturingFacility": { "@id": "dppk:manufacturingFacility", "@type": "@id" },

    // Details
    "organizationName": "dppk:organizationName",
    "tradingName": "dppk:tradingName",
    "gln": "dppk:gln",
    "leiCode": "dppk:leiCode",
    "organizationRole": "dppk:organizationRole", // Moved from redundant block
    "additionalOrganizationID": "dppk:additionalOrganizationID",
    "additionalOrganizationIDType": "dppk:additionalOrganizationIDType",
    "iso6523": "dppk:iso6523",
    "icd": "dppk:icd",
    "iso6523Value": "dppk:iso6523Value",

    // Address and Contact
    "address": "dppk:address",
    "streetAddress": "dppk:streetAddress",
    "postalCode": "dppk:postalCode",
    "addressLocality": "dppk:addressLocality",
    "addressCountry": "dppk:addressCountry",
    "email": "dppk:email",
    "telephone": "dppk:telephone",

    // ==================================================================
    // Properties: Product Description and Materials
    // ==================================================================
    
    "productName": "dppk:productName",
    "description": "dppk:description",
    "gtin": "dppk:gtin",
    "brand": "dppk:brand",
    "model": "dppk:model",
    "image": { "@id": "dppk:image", "@type": "@id" },
    "color": "dppk:color",
    "sizeCode": "dppk:sizeCode",
    "sizeSystemCode": "dppk:sizeSystemCode",
    
    // BOM and Materials
    "hasPart": { "@id": "dppk:hasPart", "@container": "@list" },
    "dppLink": { "@id": "dppk:dppLink", "@type": "@id" },
    "materialClassification": "dppk:materialClassification",
    "countryOfOrigin": "dppk:countryOfOrigin",
    "weightPercentage": "dppk:weightPercentage",
    "recycledContentPercentage": "dppk:recycledContentPercentage",

    // Quantitative Values
    // Define custom datatypes (typed literals) for measurements with implicit units.
    // This allows simple values in JSON (e.g., "netWeight": 4968) to be semantically tagged.
    "KgWeightLiteral": {
      "@id": "dppk:KgWeightLiteral",
      "@type": "@id"
    },
    "MetersLengthLiteral": {
      "@id": "dppk:MetersLengthLiteral",
      "@type": "@id"
    },

    // Coerce properties to use these custom datatypes.
    "netWeight": { "@id": "dppk:netWeight", "@type": "KgWeightLiteral" },
    "length": { "@id": "dppk:length", "@type": "MetersLengthLiteral" },
    "grossWeight": "dppk:grossWeight", // No default unit coercion for grossWeight.
    "value": { "@id": "dppk:value", "@type": "xsd:decimal" },
    "unitCode": "dppk:unitCode",
    "unitText": "dppk:unitText",

    // ==================================================================
    // Properties: Compliance and Regulatory
    // ==================================================================

    // Relationships (Data Clusters)
    "documents": { "@id": "dppk:documents", "@container": "@set" },
    "certifications": { "@id": "dppk:certifications", "@container": "@set" },
    "substancesOfConcern": { "@id": "dppk:substancesOfConcern", "@container": "@set" },
    "ecoDesignProperties": { "@id": "dppk:ecoDesignProperties", "@container": "@set" },
    "complianceTopics": { "@id": "dppk:complianceTopics", "@container": "@set" },
    "packaging": { "@id": "dppk:packaging", "@container": "@set" },
    "productionSteps": { "@id": "dppk:productionSteps", "@container": "@list" },

    // Document Details
    "documentType": "dppk:documentType",
    "languageCode": "dppk:languageCode",
    "referencedFileURL": { "@id": "dppk:referencedFileURL", "@type": "@id" }, // Moved from redundant block
    "documentURL": { "@id": "dppk:documentURL", "@type": "@id" },

    // Certification Details
    "certificationStandard": "dppk:certificationStandard",
    "certificationIdentification": "dppk:certificationIdentification",
    "certifyingBodyName": "dppk:certifyingBodyName",
    "certificationStartDate": { "@id": "dppk:certificationStartDate", "@type": "xsd:date" },
    "certifyingBodyID": "dppk:certifyingBodyID",

    // Substance Details
    "minValue": "dppk:minValue",
    "maxValue": "dppk:maxValue",

    // Eco-Design Details
    "ecoDesignPropertyType": "dppk:ecoDesignPropertyType",

    // Compliance Topic Details
    "complianceType": "dppk:complianceType",
    "complianceValue": "dppk:complianceValue",
    "complianceText": "dppk:complianceText",

    // Packaging Details
    "packagingMaterialType": "dppk:packagingMaterialType",
    "packagingMaterialCompositionQuantity": "dppk:packagingMaterialCompositionQuantity",
    "packagingRecyclingProcessType": "dppk:packagingRecyclingProcessType",
    "packagingRecycledContent": "dppk:packagingRecycledContent",
    "packagingSubstanceOfConcern": "dppk:packagingSubstanceOfConcern",

    // Production Step Details
    "dateOfManufacture": {
      "@id": "dppk:dateOfManufacture",
      "@type": "xsd:date"
    },
    "productionStepType": "dppk:productionStepType",
    "productionLocationCountry": "dppk:productionLocationCountry",

    // ==================================================================
    // General Purpose Aliases (Used across multiple classes)
    // ==================================================================
    "name": "dppk:name", // A generic name/title for non-product entities.
    // Using schema.org for generic identifiers (like CAS numbers) and URLs as they were not defined in the core jtc24 ontology.
    "identifier": "schema:identifier",
    "url": { "@id": "schema:url", "@type": "@id" }

  }
}
