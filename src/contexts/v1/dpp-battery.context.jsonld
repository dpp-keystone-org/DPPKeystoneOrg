                "@type": "@id",
                "@context": {
                    "initial": "dppk:internalResistanceInitial",
                    "increase": "dppk:internalResistanceIncrease"
                }
            },
            "lifetime": { 
                "@id": "dppk:lifetime", 
                "@type": "@id",
                "@context": {
                    "years": "dppk:expectedLifetimeYears",
                    "cycles": "dppk:expectedLifetimeCycles",
                    "cycleCountFull": "dppk:cycleCountFull",
                    "referenceTest": "dppk:referenceTestCycleLife",
                    "cRateTest": "dppk:cRateCycleTest",
                    "energyThroughput": "dppk:energyThroughput",
                    "capacityThroughput": "dppk:capacityThroughput",
                    "exhaustionThreshold": "dppk:exhaustionCapacityThreshold"
                }
            },
            "temperature": { 
                "@id": "dppk:temperature", 
                "@type": "@id",
                "@context": {
                    "idleLower": "dppk:temperatureIdleLower",
                    "idleUpper": "dppk:temperatureIdleUpper",
                    "timeExtremeUpper": "dppk:timeExtremeTempUpper",
                    "timeExtremeLower": "dppk:timeExtremeTempLower",
                    "timeChargingUpper": "dppk:timeChargingTempUpper",
                    "timeChargingLower": "dppk:timeChargingTempLower"
                }
            },
            "negativeEvents": { 
                "@id": "dppk:negativeEvents", 
                "@type": "@id",
                "@context": {
                    "deepDischarge": "dppk:eventsDeepDischarge",
                    "overcharge": "dppk:eventsOvercharge",
                    "accidents": "dppk:eventsAccidents"
                }
            }
