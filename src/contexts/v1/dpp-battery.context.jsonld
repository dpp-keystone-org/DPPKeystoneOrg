      "criticalRawMaterials": "dppk:criticalRawMaterials",
      "materialComposition": {
        "@id": "dppk:materialComposition",
        "@context": {
          "name": "dppk:componentName",
          "weightPercentage": { "@id": "dppk:percentage", "@type": "xsd:double" },
          "uniqueProductIdentifier": { "@id": "dppk:uniqueProductIdentifier", "@type": "@id" }
        }
      },
      "hazardousSubstances": {
        "@id": "dppk:hazardousSubstances",
        "@context": {
          "name": "dppk:componentName",
          "weightPercentage": { "@id": "dppk:percentage", "@type": "xsd:double" },
          "uniqueProductIdentifier": { "@id": "dppk:uniqueProductIdentifier", "@type": "@id" }
        }
      },
      "hazardousSubstancesImpact": "dppk:hazardousSubstancesImpact",
      "dismantlingInformation": { "@id": "dppk:dismantlingInformation", "@type": "@id" },
      "partNumbers": { "@id": "dppk:partNumbers", "@type": "@id" },
