{
  "@context": {
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "dppk": "https://dpp-keystone.org/spec/v1/terms#",
    "dcterms": "http://purl.org/dc/terms/",
    "schema": "http://schema.org/"
  },
  "@id": "https://dpp-keystone.org/spec/ontology/v1/sectors/Battery",
  "@type": "owl:Ontology",
  "dcterms:title": "DPP Battery Sector Ontology",
  "dcterms:description": "Defines the specific data model for the EU Battery Passport, including battery-specific properties, materials, and regulatory information.",
  "rdfs:label": [
    {
      "@language": "en",
      "@value": "DPP Battery Sector Ontology"
    }
  ],
  "rdfs:comment": [
    {
      "@language": "en",
      "@value": "Defines the specific data model for the EU Battery Passport, including battery-specific properties, materials, and regulatory information."
    }
  ],
  "owl:imports": [
    { "@id": "https://dpp-keystone.org/spec/ontology/v1/core/Product.jsonld" },
    { "@id": "https://dpp-keystone.org/spec/ontology/v1/core/EPD.jsonld" },
    { "@id": "https://dpp-keystone.org/spec/ontology/v1/core/DoPC.jsonld" },
    { "@id": "https://dpp-keystone.org/spec/ontology/v1/core/RelatedResource.jsonld" },
    { "@id": "https://dpp-keystone.org/spec/ontology/v1/core/Organization.jsonld" }
  ],
  "@graph": [
    {
      "@id": "dppk:BatteryProduct",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": { "@id": "dppk:Product" },
      "rdfs:label": [ { "@language": "en", "@value": "Battery Product" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "Data model for the Battery Passport according to EU Battery Regulation." } ]
    },
    
    // --- Sub-Classes for Structure ---
    {
      "@id": "dppk:BatteryPerformance",
      "@type": "rdfs:Class",
      "dppk:governedBy": "Annex XIII (1)",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "Performance and Durability" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "Container for performance and durability parameters of the battery." } ]
    },
    {
      "@id": "dppk:CapacityInfo",
      "@type": "rdfs:Class",
      "dppk:governedBy": "Annex XIII (1g, 1h, 4d)",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "Capacity, Energy and Voltage" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "Information regarding the battery's capacity, energy, state of charge, and voltage limits." } ]
    },
    {
      "@id": "dppk:PowerInfo",
      "@type": "rdfs:Class",
      "dppk:governedBy": "Annex XIII (1i); Art. 10",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "Power Capability" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "Information regarding the battery's power capability, power fade, and maximum permitted power." } ]
    },
    {
      "@id": "dppk:EfficiencyInfo",
      "@type": "rdfs:Class",
      "dppk:governedBy": "Annex XIII (1n); Art. 10",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "Round Trip Efficiency and Self-Discharge" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "Information regarding the battery's round trip energy efficiency and self-discharge rates." } ]
    },
    {
      "@id": "dppk:InternalResistanceInfo",
      "@type": "rdfs:Class",
      "dppk:governedBy": "Annex XIII (1o); Art. 10",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "Internal Resistance" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "Information regarding the battery's internal resistance and its increase over time." } ]
    },
    {
      "@id": "dppk:LifetimeInfo",
      "@type": "rdfs:Class",
      "dppk:governedBy": "Annex XIII (1j, 1k, 4a); Annex IV",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "Battery Lifetime" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "Information regarding the expected lifetime of the battery in years and cycles, including reference tests." } ]
    },
    {
      "@id": "dppk:TemperatureInfo",
      "@type": "rdfs:Class",
      "dppk:governedBy": "Annex XIII (1l); Annex VII Part B (4)",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "Temperature Conditions" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "Information regarding the temperature ranges the battery can withstand and time spent in extreme temperatures." } ]
    },
    {
      "@id": "dppk:NegativeEventsInfo",
      "@type": "rdfs:Class",
      "dppk:governedBy": "Annex XIII (4d); Annex VII Part B (4)",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "Negative Events and Accidents" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "Information regarding negative events such as deep discharges, overcharges, and accidents." } ]
    },

    // --- Identifiers & Core Data ---
    {
      "@id": "dppk:manufacturerInfo",
      "@type": "owl:ObjectProperty",
      "dppk:governedBy": "Annex VI Part A (1)",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "Manufacturer identifier and information" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "A battery passport must include information identifying the manufacturer. The manufacturer information must also be provided on the battery label. Includes name, registered trade name/mark, postal address, web address, and email." } ],
      "rdfs:domain": { "@id": "dppk:BatteryProduct" },
      "rdfs:range": { "@id": "dppk:Organization" }
    },
    {
      "@id": "dppk:manufacturingPlace",
      "@type": "owl:DatatypeProperty",
      "dppk:governedBy": "Annex XIII (1a)",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "Manufacturing place" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "A battery passport must include the manufacturing place (geographical location of a battery manufacturing plant). The manufacturing place must also be provided on the battery label." } ],
      "rdfs:domain": { "@id": "dppk:BatteryProduct" },
      "rdfs:range": "xsd:string"
    },
    {
      "@id": "dppk:manufacturingDate",
      "@type": "owl:DatatypeProperty",
      "dppk:governedBy": "Annex XIII (1a)",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "Manufacturing date" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "A battery passport must include the date of manufacture (month and year). The manufacturing date must also be provided on the battery label. Format YYYY-MM-DD (ISO 8601)." } ],
      "rdfs:domain": { "@id": "dppk:BatteryProduct" },
      "rdfs:range": "xsd:date"
    },
    {
      "@id": "dppk:serviceDate",
      "@type": "owl:DatatypeProperty",
      "dppk:governedBy": "Annex VII Part B (1)",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "Date of putting the battery into service" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "Where appropriate, the battery passport must include information on the date of putting the battery into service. Format YYYY-MM-DD (ISO 8601)." } ],
      "rdfs:domain": { "@id": "dppk:BatteryProduct" },
      "rdfs:range": "xsd:date"
    },
    {
      "@id": "dppk:warrantyPeriod",
      "@type": "owl:DatatypeProperty",
      "dppk:governedBy": "Annex XIII (1m)",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "Warranty period of the battery" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "The battery passport must include information about the period for which the commercial warranty applies." } ],
      "rdfs:domain": { "@id": "dppk:BatteryProduct" },
      "rdfs:range": "xsd:string"
    },
    {
      "@id": "dppk:batteryCategory",
      "@type": "owl:DatatypeProperty",
      "dppk:governedBy": "Annex XIII (1a); Annex VI Part A (2)",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "Battery category" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "A battery passport must include the battery category. The battery category must be provided on the battery label. Categories: LMT battery, electric vehicle battery, or industrial battery." } ],
      "rdfs:domain": { "@id": "dppk:BatteryProduct" },
      "rdfs:range": "xsd:string"
    },
    {
      "@id": "dppk:batteryMass",
      "@type": "owl:DatatypeProperty",
      "dppk:governedBy": "Annex XIII (1a); Annex VI Part A (5)",
      "dppk:unit": "kg",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "Battery mass" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "A battery passport must include the battery mass. The battery mass must also be provided on the battery label." } ],
      "rdfs:domain": { "@id": "dppk:BatteryProduct" },
      "rdfs:range": "xsd:double"
    },
    {
      "@id": "dppk:batteryStatus",
      "@type": "owl:DatatypeProperty",
      "dppk:governedBy": "Annex XIII 4(c)",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "Battery status" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "A battery passport must include information on the status of the battery. Statuses: original, repurposed, re-used, remanufactured, or waste." } ],
      "rdfs:domain": { "@id": "dppk:BatteryProduct" },
      "rdfs:range": "xsd:string"
    },

    // --- Symbols & Labels ---
    {
      "@id": "dppk:separateCollectionSymbol",
      "@type": "owl:ObjectProperty",
      "dppk:governedBy": "Annex XIII (1s); Art. 13(4); Annex VI Part B",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "Separate collection symbol" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "The battery passport must include the symbol for separate collection. The symbol should be translated into text to ensure machine readability." } ],
      "rdfs:domain": { "@id": "dppk:BatteryProduct" },
      "rdfs:range": { "@id": "dppk:RelatedResource" }
    },
    {
      "@id": "dppk:heavyMetalSymbols",
      "@type": "owl:ObjectProperty",
      "dppk:governedBy": "Annex XIII (1s); Art. 13(5)",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "Symbols for cadmium and lead" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "Batteries containing more than 0.002% cadmium or 0.004% lead must be marked with the chemical symbol (Cd or Pb)." } ],
      "rdfs:domain": { "@id": "dppk:BatteryProduct" },
      "rdfs:range": { "@id": "dppk:RelatedResource" }
    },
    {
      "@id": "dppk:carbonFootprintLabel",
      "@type": "owl:ObjectProperty",
      "dppk:governedBy": "Article 7(2) via Annex XIII (1c)",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "Carbon footprint label" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "EV and rechargeable industrial batteries > 2 kWh and LMT batteries must bear a label indicating the carbon footprint and performance class." } ],
      "rdfs:domain": { "@id": "dppk:BatteryProduct" },
      "rdfs:range": { "@id": "dppk:RelatedResource" }
    },
    {
      "@id": "dppk:extinguishingAgent",
      "@type": "owl:DatatypeProperty",
      "dppk:governedBy": "Annex VI Part A (9)",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "Extinguishing agent" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "A battery passport must include the usable extinguishing agent. The information should be related to their possible area of application as regulated in DIN EN 2." } ],
      "rdfs:domain": { "@id": "dppk:BatteryProduct" },
      "rdfs:range": "xsd:string"
    },
    {
      "@id": "dppk:labelMeaning",
      "@type": "owl:DatatypeProperty",
      "dppk:governedBy": "Annex XIII (1s); Art. 74 1(e)",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "Meaning of labels and symbols" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "Information regarding the prevention and management of waste batteries including the meaning of the labels and symbols." } ],
      "rdfs:domain": { "@id": "dppk:BatteryProduct" },
      "rdfs:range": "xsd:string"
    },

    // --- Carbon Footprint (Extras not in EPD) ---
    {
      "@id": "dppk:testReports",
      "@type": "owl:ObjectProperty",
      "dppk:governedBy": "Annex XIII (3); Annex VIII Part A 2(h)",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "Results of test reports proving compliance" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "A battery passport must include the test report results that can prove the compliance with the requirements stated in the battery regulation." } ],
      "rdfs:domain": { "@id": "dppk:BatteryProduct" },
      "rdfs:range": { "@id": "dppk:RelatedResource" }
    },
    {
      "@id": "dppk:carbonFootprintClass",
      "@type": "owl:DatatypeProperty",
      "dppk:governedBy": "Annex XIII (1c); Art. 7(2)",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "Carbon footprint performance class" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "The battery passport must include the carbon footprint performance class in accordance with the delegated and implementing acts." } ],
      "rdfs:domain": { "@id": "dppk:BatteryProduct" },
      "rdfs:range": "xsd:string"
    },
    {
      "@id": "dppk:carbonFootprintStudy",
      "@type": "owl:ObjectProperty",
      "dppk:governedBy": "Annex XIII (1c); Art. 7 1(g)",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "Web link to public carbon footprint study" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "The battery passport must include a web link to the carbon footprint study accompanying the battery carbon footprint declaration." } ],
      "rdfs:domain": { "@id": "dppk:BatteryProduct" },
      "rdfs:range": { "@id": "dppk:RelatedResource" }
    },
    {
      "@id": "dppk:carbonFootprintGeneralInfo",
      "@type": "owl:ObjectProperty",
      "dppk:governedBy": "Annex XIII, Article 7(1)",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "General battery and manufacturer information" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "The battery passport must contain general battery and manufacturer information in relation to the battery carbon footprint." } ],
      "rdfs:domain": { "@id": "dppk:BatteryProduct" },
      "rdfs:range": { "@id": "dppk:RelatedResource" }
    },
    {
      "@id": "dppk:carbonFootprintAbsolute",
      "@type": "owl:DatatypeProperty",
      "dppk:unit": "tCO2e",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "Absolute battery carbon footprint" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "The battery passport should include the battery carbon footprint in absolute terms (kg CO2e)." } ],
      "rdfs:domain": { "@id": "dppk:BatteryProduct" },
      "rdfs:range": "xsd:double"
    },

    // --- Supply Chain ---
    {
      "@id": "dppk:dueDiligenceReport",
      "@type": "owl:ObjectProperty",
      "dppk:governedBy": "Art. 52(3)",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "Information of due diligence report" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "The battery passport must include information on responsible sourcing as indicated in the report on its due diligence policy." } ],
      "rdfs:domain": { "@id": "dppk:BatteryProduct" },
      "rdfs:range": { "@id": "dppk:RelatedResource" }
    },
    {
      "@id": "dppk:supplyChainAssurances",
      "@type": "owl:ObjectProperty",
      "dppk:governedBy": "Art. 49(d)",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "Third party assurances of recognised schemes" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "The battery passport should contain information on third-party supply chain assurances such as certifications." } ],
      "rdfs:domain": { "@id": "dppk:BatteryProduct" },
      "rdfs:range": { "@id": "dppk:RelatedResource" }
    },
    {
      "@id": "dppk:supplyChainIndices",
      "@type": "owl:ObjectProperty",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "Supply chain indices" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "The battery passport should contain supply chain indices to allow easy assessment for consumers on level of responsible sourcing." } ],
      "rdfs:domain": { "@id": "dppk:BatteryProduct" },
      "rdfs:range": { "@id": "dppk:RelatedResource" }
    },

    // --- Materials & Composition ---
    {
      "@id": "dppk:batteryChemistry",
      "@type": "owl:DatatypeProperty",
      "dppk:governedBy": "Annex XIII (1b); Annex VI Part A (7)",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "Battery chemistry" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "The battery passport must contain information on the battery chemistry (cathode, anode, electrolyte)." } ],
      "rdfs:domain": { "@id": "dppk:BatteryProduct" },
      "rdfs:range": "xsd:string"
    },
    {
      "@id": "dppk:criticalRawMaterials",
      "@type": "owl:DatatypeProperty",
      "dppk:governedBy": "Annex XIII (1b); Annex VI Part A (10)",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "Critical raw materials" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "The battery passport must contain information on the critical raw materials present in the battery in a concentration of more than 0.1%." } ],
      "rdfs:domain": { "@id": "dppk:BatteryProduct" },
      "rdfs:range": "xsd:string"
    },
    {
      "@id": "dppk:materialComposition",
      "@type": "owl:ObjectProperty",
      "dppk:governedBy": "Annex XIII (2a)",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "Materials used in cathode, anode and electrolyte" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "The battery passport must contain information on the detailed composition of the battery, including materials used in the cathode, anode and electrolyte. Reuses dppk:Component." } ],
      "rdfs:domain": { "@id": "dppk:BatteryProduct" },
      "rdfs:range": { "@id": "dppk:Component" }
    },
    {
      "@id": "dppk:hazardousSubstances",
      "@type": "owl:ObjectProperty",
      "dppk:governedBy": "Annex XIII (1b); Annex VI Part A (8)",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "Hazardous substances" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "The battery passport must contain information on hazardous substances other than mercury, cadmium, or lead. Reuses dppk:Component." } ],
      "rdfs:domain": { "@id": "dppk:BatteryProduct" },
      "rdfs:range": { "@id": "dppk:Component" }
    },
    {
      "@id": "dppk:hazardousSubstancesImpact",
      "@type": "owl:DatatypeProperty",
      "dppk:governedBy": "Annex XIII (1s); Art. 74 1(f)",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "Impact of substances on environment, human health, safety, persons" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "The battery passport must contain information on the impact of substances, in particular hazardous substances, on the environment and human health." } ],
      "rdfs:domain": { "@id": "dppk:BatteryProduct" },
      "rdfs:range": "xsd:string"
    },

    // --- Circularity ---
    {
      "@id": "dppk:dismantlingInformation",
      "@type": "owl:ObjectProperty",
      "dppk:governedBy": "Annex XIII (2c)",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
      "rdfs:label": [ { "@language": "en", "@value": "Dismantling information" } ],
      "rdfs:comment": [ { "@language": "en", "@value": "A battery passport must include dismantling information, including exploded diagrams, disassembly sequences, and tools required." } ],
      "rdfs:domain": { "@id": "dppk:BatteryProduct" },
      "rdfs:range": { "@id": "dppk:RelatedResource" }
    },
    {
      "@id": "dppk:partNumbers",
      "@type": "owl:ObjectProperty",
      "dppk:governedBy": "Annex XIII (2b)",
      "dcterms:source": { "@id": "https://www.dinmedia.de/en/standard/din-dke-spec-99100" },
